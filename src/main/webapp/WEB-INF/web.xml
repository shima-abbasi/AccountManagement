<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>CreateRealCustomerServlet</servlet-name>
    <servlet-class>servlets.CreateRealCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CreateLegalCustomerServlet</servlet-name>
    <servlet-class>servlets.CreateLegalCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchLegalCustomerServlet</servlet-name>
    <servlet-class>servlets.SearchLegalCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchRealCustomerServlet</servlet-name>
    <servlet-class>servlets.SearchRealCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteRealCustomerServlet</servlet-name>
    <servlet-class>servlets.DeleteRealCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteLegalCustomerServlet</servlet-name>
    <servlet-class>servlets.DeleteLegalCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RetrieveLegalCustomerServlet</servlet-name>
    <servlet-class>servlets.RetrieveLegalCustomerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateLegalCustomerServlet</servlet-name>
    <servlet-class>servlets.UpdateLegalCustomerServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>CreateRealCustomerServlet</servlet-name>
    <url-pattern>/CreateRealCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CreateLegalCustomerServlet</servlet-name>
    <url-pattern>/CreateLegalCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchLegalCustomerServlet</servlet-name>
    <url-pattern>/SearchLegalCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchRealCustomerServlet</servlet-name>
    <url-pattern>/SearchRealCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteRealCustomerServlet</servlet-name>
    <url-pattern>/DeleteRealCustomerServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>DeleteLegalCustomerServlet</servlet-name>
    <url-pattern>/DeleteLegalCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RetrieveLegalCustomerServlet</servlet-name>
    <url-pattern>/RetrieveLegalCustomerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateLegalCustomerServlet</servlet-name>
    <url-pattern>/UpdateLegalCustomerServlet</url-pattern>
  </servlet-mapping>



</web-app>
